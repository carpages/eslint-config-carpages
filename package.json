{
  "name": "eslint-config-carpages",
  "version": "3.1.0",
  "description": "A shareable Eslint config for use by Carpages.ca developers.",
  "main": "index.js",
  "scripts": {
    "test": "ava -v",
    "eslint-check": "eslint --print-config .eslintrc.js",
    "format": "prettier-eslint --write ./**/*.js --eslint-config-path ./.eslintrc.js --ignore **/node_modules/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carpages/eslint-config-carpages"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Jeff Andrews <jeffandrews@me.com> (http://jeffwandrews.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carpages/eslint-config-carpages/issues"
  },
  "homepage": "https://github.com/carpages/eslint-config-carpages",
  "peerDependencies": {
    "eslint": ">= 4.x"
  },
  "dependencies": {
    "babel-eslint": "8.x",
    "eslint-config-standard": "11.x",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-requirejs": "^3.2.0",
    "eslint-plugin-standard": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "4.x",
    "eslint-config-carpages": "./",
    "prettier-eslint-cli": "^4.7.1"
  }
}
