    "url": "https://github.com/carpages/eslint-config-carpages"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Jeff Andrews <jeffandrews@me.com> (http://jeffwandrews.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carpages/eslint-config-carpages/issues"
  },
  "homepage": "https://github.com/carpages/eslint-config-carpages",
  "peerDependencies": {
    "babel-eslint": "10.x",
    "eslint": ">= 4.x",
    "eslint-config-standard": "12.x",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-node": "8.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-requirejs": "3.x",
    "eslint-plugin-standard": "4.x"
  },
  "devDependencies": {
    "ava": "1.x",
    "babel-eslint": "10.x",
    "eslint": "5.x",
    "eslint-config-carpages": "./",
    "eslint-config-standard": "12.x",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-node": "8.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-requirejs": "3.x",
    "eslint-plugin-standard": "4.x",
    "prettier-eslint-cli": "4.x"
  }
