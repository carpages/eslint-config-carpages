{
  "name": "eslint-config-carpages",
  "version": "3.2.0-alpha.1",
  "description": "A shareable Eslint config for use by Carpages.ca developers.",
  "main": "index.js",
  "scripts": {
    "test": "ava -v",
    "eslint-check": "eslint --print-config .eslintrc.js",
